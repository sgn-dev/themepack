{
  "name":"sgn47gradnord/themepack",
  "description":"Customized Extension to use Theme working on Contao 4.13+ with PHP 8.3 support",
  "type":"contao-bundle",
  "license":"proprietary",
  "authors":[
    {
      "name":"47 GradNord - Agentur für Internetlösungen",
      "homepage":"http://www.47gradnord.de"
    }
  ],
  "require":{
    "php": "^7.4 || ^8.0",
    "contao/core-bundle":"^4.13"
  },
  "require-dev":{
    "contao/manager-plugin": "^2.0",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload":{
    "psr-4": {
      "Sgn47gradnord\\Themepack\\": "src/"
    }
  },
  "extra":{
    "contao-manager-plugin": "Sgn47gradnord\\Themepack\\ContaoManager\\Plugin"
  }
}